{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "report-editor-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/reportEditor/ReportEditorWebPart.js",
          "manifest": "./src/webparts/reportEditor/ReportEditorWebPart.manifest.json"
        }
      ]
    },
    "report-rotator-web-part": {
      "components": [
