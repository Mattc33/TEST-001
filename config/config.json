{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "report-viewer-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/reportViewer/ReportViewerWebPart.js",
          "manifest": "./src/webparts/reportViewer/ReportViewerWebPart.manifest.json"
        }
      ]
    },
    "report-rotator-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/reportRotator/ReportRotatorWebPart.js",
          "manifest": "./src/webparts/reportRotator/ReportRotatorWebPart.manifest.json"
        }
      ]
    },
    "search-box-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/searchBox/SearchBoxWebPart.js",
          "manifest": "./src/webparts/searchBox/SearchBoxWebPart.manifest.json"
        }
      ]
    },
    "search-results-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/searchResults/SearchResultsWebPart.js",
          "manifest": "./src/webparts/searchResults/SearchResultsWebPart.manifest.json"
        }
      ]
    },
    "report-my-fav-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/reportMyFav/ReportMyFavWebPart.js",
          "manifest": "./src/webparts/reportMyFav/ReportMyFavWebPart.manifest.json"
        }
      ]
    },
    "data-marketplace-renderer-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/dataMarketplaceRenderer/DataMarketplaceRendererApplicationCustomizer.js",
          "manifest": "./src/extensions/dataMarketplaceRenderer/DataMarketplaceRendererApplicationCustomizer.manifest.json"
        }
      ]
    },
    "report-comments-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/reportComments/ReportCommentsWebPart.js",
          "manifest": "./src/webparts/reportComments/ReportCommentsWebPart.manifest.json"
        }
      ]
    },
    "sysco-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/sysco/SyscoApplicationCustomizer.js",
          "manifest": "./src/extensions/sysco/SyscoApplicationCustomizer.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "ReportViewerWebPartStrings": "lib/webparts/reportViewer/loc/{locale}.js",
    "ReportEditorWebPartStrings": "lib/webparts/reportEditor/loc/{locale}.js",
    "ReportRotatorWebPartStrings": "lib/webparts/reportRotator/loc/{locale}.js",
    "SearchBoxWebPartStrings": "lib/webparts/searchBox/loc/{locale}.js",
    "SearchResultsWebPartStrings": "lib/webparts/searchResults/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js",
    "ReportMyFavWebPartStrings": "lib/webparts/reportMyFav/loc/{locale}.js",
    "DataMarketplaceRendererApplicationCustomizerStrings": "lib/extensions/dataMarketplaceRenderer/loc/{locale}.js",
    "ReportCommentsWebPartStrings": "lib/webparts/reportComments/loc/{locale}.js",
    "SyscoApplicationCustomizerStrings": "lib/extensions/sysco/loc/{locale}.js"
  }
}
